server {
       listen 80;
       listen [::]:80;

       server_name wosee.ddns.net;

       root /var/www/nwsync;
       index index.html;

       location / {
               root /var/www/nwsync;
               index index.html;
               autoindex on;
               autoindex_exact_size off;
               try_files $uri $uri/ =404;
       }
}
