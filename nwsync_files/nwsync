server {
       listen 80;
       listen [::]:80;

       server_name 192.168.1.1;

       root /var/www/nwsync;
       index index.html;

       location / {
               proxy_pass no.ip.address;
               root /var/www/nwsync;
               index index.html;
               autoindex on;
               autoindex_exact_size off;
               try_files $uri $uri/ =404;
       }
}
